<Page x:Class="Round_Minecraft_Launcher.Pages.Main_SubPages.Download_Java"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Round_Minecraft_Launcher.Pages.Main_SubPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Download">

    <Grid x:Name="version_list">
        <ListBox x:Name="list_ver" HorizontalContentAlignment="Stretch" Background="{x:Null}" HorizontalAlignment="Center">
            <Grid x:Name="test" Height="50" Width="584" VerticalAlignment="Top">
                <Button Content="Button" Margin="0,0,10,0" Height="30" HorizontalAlignment="Right"/>
                <Label Content="Version" Margin="87,8,84,8" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" HorizontalContentAlignment="Center" FontSize="16" VerticalContentAlignment="Center"/>
                <Grid HorizontalAlignment="Left" Width="82" Margin="5,0,0,0">
                    <Grid.Background>
                        <ImageBrush ImageSource="/Minecraft_Version.png" Stretch="Uniform"/>
                    </Grid.Background>
                </Grid>
            </Grid>
        </ListBox>

    </Grid>
</Page>
